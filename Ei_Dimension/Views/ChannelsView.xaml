<UserControl x:Class="Ei_Dimension.Views.ChannelsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
             xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
             xmlns:ViewModels="clr-namespace:Ei_Dimension.ViewModels"
             mc:Ignorable="d"
             DataContext="{dxmvvm:ViewModelSource Type=ViewModels:ChannelsViewModel}"
             d:DesignHeight="674" d:DesignWidth="1312">
  <Grid>
    <TextBlock Text="Channels"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="50"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Center"
                 VerticalAlignment="Top"
                 Margin="0,10,250,0"/>


    <!--Motors Parameters-->
    <Border HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Margin="60,110,0,00">

      <StackPanel Orientation="Horizontal"
                  HorizontalAlignment="Stretch"
                  VerticalAlignment="Stretch"
                  Margin="10,0,10,0">
        <StackPanel VerticalAlignment="Stretch"
                    Margin="0,40,3,0">
          <TextBlock Text="Green A (SSC)"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="20"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,0,8,0"/>
          <TextBlock Text="Green B (PE 2%)"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="20"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,15,4,0"/>
          <TextBlock Text="Green C PE"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="20"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,16,4,0"/>
          <TextBlock Text="Red A (CL3)"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="20"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,15,4,0"/>
          <TextBlock Text="Red B (SSC)"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="20"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,15,4,0"/>
          <TextBlock Text="Red C (CL1)"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="20"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,15,4,0"/>
          <TextBlock Text="Red D (CL2)"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="20"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,15,4,0"/>
          <TextBlock Text="Violet A (SSC)"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="20"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,15,4,0"/>
          <TextBlock Text="Violet B (CL0)"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="20"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,15,4,0"/>
          <TextBlock Text="Forward Scatter"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="20"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,15,4,0"/>
        </StackPanel>
        <StackPanel VerticalAlignment="Stretch"
                    Width="120"
                    Margin="0,0,20,0">
          <TextBlock Text="Bias @ 30ᴼC"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="20"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Center"
                 VerticalAlignment="Top"
                 Margin="0,0,0,10"/>

          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding Bias30Parameters[0]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding Bias30Parameters[1]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding Bias30Parameters[2]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding Bias30Parameters[3]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding Bias30Parameters[4]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding Bias30Parameters[5]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding Bias30Parameters[6]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding Bias30Parameters[7]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding Bias30Parameters[8]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding Bias30Parameters[9]}"/>
        </StackPanel>
        <StackPanel VerticalAlignment="Stretch"
                    Width="120"
                    Margin="0,0,15,0">
          <TextBlock Text="Tcomp Bias"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="20"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Center"
                 VerticalAlignment="Top"
                 Margin="0,0,0,10"/>

          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TcompBiasParameters[0]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TcompBiasParameters[1]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TcompBiasParameters[2]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TcompBiasParameters[3]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TcompBiasParameters[4]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TcompBiasParameters[5]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TcompBiasParameters[6]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TcompBiasParameters[7]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TcompBiasParameters[8]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TcompBiasParameters[9]}"/>
        </StackPanel>
        <StackPanel VerticalAlignment="Stretch"
                    Width="120"
                    Margin="0,0,15,0">
          <TextBlock Text="Temp"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="20"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Center"
                 VerticalAlignment="Top"
                 Margin="0,0,0,10"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TempParameters[0]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TempParameters[1]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TempParameters[2]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TempParameters[3]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TempParameters[4]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TempParameters[5]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TempParameters[6]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TempParameters[7]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TempParameters[8]}"/>
          <TextBox  Height="36"
                      HorizontalAlignment="Stretch"
                      Margin="0,0,0,3"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding TempParameters[9]}"/>
        </StackPanel>
        <StackPanel VerticalAlignment="Stretch"
                    Width="120"
                    Margin="0,0,15,0">
          <TextBlock Text="Background"
                     FontFamily="{StaticResource AppTextFont}"
                     FontSize="20"
                     FontWeight="UltraBold"
                     Foreground="{StaticResource AppTextColor}"
                     HorizontalAlignment="Center"
                     VerticalAlignment="Top"
                     Margin="0,0,0,10"/>
          <TextBox  Height="36"
                    HorizontalAlignment="Stretch"
                    Margin="0,0,0,3"
                    Style="{StaticResource InputFieldStyle}"
                    Text="{Binding BackgroundParameters[0]}"/>
          <TextBox  Height="36"
                    HorizontalAlignment="Stretch"
                    Margin="0,0,0,3"
                    Style="{StaticResource InputFieldStyle}"
                    Text="{Binding BackgroundParameters[1]}"/>
          <TextBox  Height="36"
                    HorizontalAlignment="Stretch"
                    Margin="0,0,0,3"
                    Style="{StaticResource InputFieldStyle}"
                    Text="{Binding BackgroundParameters[2]}"/>
          <TextBox  Height="36"
                    HorizontalAlignment="Stretch"
                    Margin="0,0,0,3"
                    Style="{StaticResource InputFieldStyle}"
                    Text="{Binding BackgroundParameters[3]}"/>
          <TextBox  Height="36"
                    HorizontalAlignment="Stretch"
                    Margin="0,0,0,3"
                    Style="{StaticResource InputFieldStyle}"
                    Text="{Binding BackgroundParameters[4]}"/>
          <TextBox  Height="36"
                    HorizontalAlignment="Stretch"
                    Margin="0,0,0,3"
                    Style="{StaticResource InputFieldStyle}"
                    Text="{Binding BackgroundParameters[5]}"/>
          <TextBox  Height="36"
                    HorizontalAlignment="Stretch"
                    Margin="0,0,0,3"
                    Style="{StaticResource InputFieldStyle}"
                    Text="{Binding BackgroundParameters[6]}"/>
          <TextBox  Height="36"
                    HorizontalAlignment="Stretch"
                    Margin="0,0,0,3"
                    Style="{StaticResource InputFieldStyle}"
                    Text="{Binding BackgroundParameters[7]}"/>
          <TextBox  Height="36"
                    HorizontalAlignment="Stretch"
                    Margin="0,0,0,3"
                    Style="{StaticResource InputFieldStyle}"
                    Text="{Binding BackgroundParameters[8]}"/>
          <TextBox  Height="36"
                    HorizontalAlignment="Stretch"
                    Margin="0,0,0,3"
                    Style="{StaticResource InputFieldStyle}"
                    Text="{Binding BackgroundParameters[9]}"/>
        </StackPanel>
        <StackPanel VerticalAlignment="Stretch"
                    Width="220">
          <TextBlock Text="SiPM Temp&#x0a;Coefficient"
                     FontFamily="{StaticResource AppTextFont}"
                     FontSize="20"
                     FontWeight="UltraBold"
                     Foreground="{StaticResource AppTextColor}"
                     HorizontalAlignment="Center"
                     VerticalAlignment="Top"
                     Margin="0,30,0,0"/>
          <TextBox  Height="36"
                    HorizontalAlignment="Stretch"
                    Margin="50,-3,50,13"
                    Style="{StaticResource InputFieldStyle}"
                    Text="{Binding SiPMTempCoeff}"/>
          <dx:SimpleButton Content="Update&#x0a;   Bias"
                             Command="{Binding UpdateBiasButtonClickCommand}"
                             Height="100"
                             FontFamily="{StaticResource AppTextFont}"
                             FontSize="36"
                             FontWeight="UltraBold"
                             Foreground="{StaticResource AppTextColor}"
                             Margin="0,0,0,6"/>
          <dx:SimpleButton Content=" Save Bias&#x0a;To Current&#x0a;       Map"
                             Command="{Binding SaveBiasButtonClickCommand}"
                             Height="150"
                             FontFamily="{StaticResource AppTextFont}"
                             FontSize="36"
                             FontWeight="UltraBold"
                             Foreground="{StaticResource AppTextColor}"
                             Margin="0,0,0,3"/>
          <TextBlock Text="{Binding CurrentMapName[0]}"
                     FontFamily="{StaticResource AppTextFont}"
                     FontSize="15"
                     FontWeight="UltraBold"
                     Foreground="{StaticResource AppTextColor}"
                     HorizontalAlignment="Center"
                     VerticalAlignment="Top"
                     Margin="0,2,0,0"/>
        </StackPanel>
      </StackPanel>
    </Border>
  </Grid>
</UserControl>
