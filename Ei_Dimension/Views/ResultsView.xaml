          </DataGrid.Columns>
        </DataGrid>
        <StackPanel Orientation="Horizontal"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Margin="34,445,0,0">
          <StackPanel Orientation="Horizontal"
                      Visibility="{Binding Buttons384Visible}">
            <RadioButton Width="100" Height="70"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Top"
                       IsChecked="{Binding CornerButtonsChecked[0], Mode=OneWay}"
                       img:EmbeddedImg.Image="/Icons/TopLeftPlate.png"
                       Style="{StaticResource ResultsRadioButtonStyle}"
                       Command="{Binding CornerButtonClickCommand}">
              <RadioButton.CommandParameter>
                <System:Int32>1</System:Int32>
              </RadioButton.CommandParameter>
            </RadioButton>
              <RadioButton Width="100" Height="70"
                       Margin="10,0,0,0"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Top"
                       IsChecked="{Binding CornerButtonsChecked[1], Mode=OneWay}"
                       img:EmbeddedImg.Image="/Icons/TopRightPlate.png"
                       Style="{StaticResource ResultsRadioButtonStyle}"
                       Command="{Binding CornerButtonClickCommand}">
              <RadioButton.CommandParameter>
                <System:Int32>2</System:Int32>
              </RadioButton.CommandParameter>
            </RadioButton>
            <RadioButton Width="100" Height="70"
                       Margin="10,0,0,0"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Top"
                       IsChecked="{Binding CornerButtonsChecked[2], Mode=OneWay}"
                       img:EmbeddedImg.Image="/Icons/BottomLeftPlate.png"
                       Style="{StaticResource ResultsRadioButtonStyle}"
                       Command="{Binding CornerButtonClickCommand}">
              <RadioButton.CommandParameter>
                <System:Int32>3</System:Int32>
              </RadioButton.CommandParameter>
            </RadioButton>
            <RadioButton Width="100" Height="70"
                       Margin="10,0,0,0"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Top"
                       IsChecked="{Binding CornerButtonsChecked[3], Mode=OneWay}"
                       img:EmbeddedImg.Image="/Icons/BottomRightPlate.png"
                       Style="{StaticResource ResultsRadioButtonStyle}"
                       Command="{Binding CornerButtonClickCommand}">
              <RadioButton.CommandParameter>
                <System:Int32>4</System:Int32>
              </RadioButton.CommandParameter>
            </RadioButton>
          </StackPanel>
          <Button Width="163" Height="70"
                  Margin="10,0,0,0"
                  HorizontalAlignment="Left"
                  VerticalAlignment="Top"
                  Content="To Current"
                  Style="{StaticResource ButtonStyle}"
                  Command="{Binding ToCurrentButtonClickCommand}"/>
        </StackPanel>
      </Grid>
    </Border>
  </Grid>
</UserControl>
