                       FontFamily="{StaticResource AppTextFont}"
                       FontSize="26"
                       FontWeight="UltraBold"
                       Foreground="{StaticResource AppTextColor}"
                       HorizontalAlignment="Right"
                       VerticalAlignment="Top"
                       Margin="0,0,10,0"/>
            <TextBlock Text="{lang:Loc Motors_A12}"
                       FontFamily="{StaticResource AppTextFont}"
                       FontSize="26"
                       FontWeight="UltraBold"
                       Foreground="{StaticResource AppTextColor}"
                       HorizontalAlignment="Right"
                       VerticalAlignment="Top"
                       Margin="0,8,10,0"/>
            <TextBlock Text="{lang:Loc Motors_H1}"
                       FontFamily="{StaticResource AppTextFont}"
                       FontSize="26"
                       FontWeight="UltraBold"
                       Foreground="{StaticResource AppTextColor}"
                       HorizontalAlignment="Right"
                       VerticalAlignment="Top"
                       Margin="0,9,10,0"/>
            <TextBlock Text="{lang:Loc Motors_H12}"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="26"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,9,10,0"/>
            <TextBlock Text="{lang:Loc Tube}"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="26"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,9,10,0"/>
          </StackPanel>
          <StackPanel VerticalAlignment="Stretch"
                      Width="100"
                      Margin="0,0,3,0">
            <TextBlock Text="{lang:Loc Z}"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="30"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Center"
                 VerticalAlignment="Top"
                 Margin="0,0,0,0"/>
            <Border Background="{StaticResource RibbonBackgroundActive}">
              <StackPanel>
                <TextBox HorizontalAlignment="Stretch"
                         Margin="8,8,8,0"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding StepsParametersZ[0]}">
                  <dxmvvm:Interaction.Behaviors>
                    <dxmvvm:EventToCommand EventName="GotFocus" Command="{Binding FocusedBoxCommand}" CommandParameter="28"/>
                  </dxmvvm:Interaction.Behaviors>
                </TextBox>
                <TextBox HorizontalAlignment="Stretch"
                         Margin="8,8,8,0"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding StepsParametersZ[1]}">
                  <dxmvvm:Interaction.Behaviors>
                    <dxmvvm:EventToCommand EventName="GotFocus" Command="{Binding FocusedBoxCommand}" CommandParameter="29"/>
                  </dxmvvm:Interaction.Behaviors>
                </TextBox>
                <TextBox HorizontalAlignment="Stretch"
                         Margin="8,8,8,0"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding StepsParametersZ[2]}">
                  <dxmvvm:Interaction.Behaviors>
                    <dxmvvm:EventToCommand EventName="GotFocus" Command="{Binding FocusedBoxCommand}" CommandParameter="30"/>
                  </dxmvvm:Interaction.Behaviors>
                </TextBox>
                <TextBox HorizontalAlignment="Stretch"
                         Margin="8,8,8,0"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding StepsParametersZ[3]}">
                  <dxmvvm:Interaction.Behaviors>
                    <dxmvvm:EventToCommand EventName="GotFocus" Command="{Binding FocusedBoxCommand}" CommandParameter="31"/>
                  </dxmvvm:Interaction.Behaviors>
                </TextBox>
                <TextBox HorizontalAlignment="Stretch"
                         Margin="8,8,8,8"
                         Style="{StaticResource InputFieldStyle}"
                         Text="{Binding StepsParametersZ[4]}">
                  <dxmvvm:Interaction.Behaviors>
                    <dxmvvm:EventToCommand EventName="GotFocus" Command="{Binding FocusedBoxCommand}" CommandParameter="32"/>
                  </dxmvvm:Interaction.Behaviors>
                </TextBox>
              </StackPanel>
            </Border>
          </StackPanel>
        </StackPanel>
      </StackPanel>
    </Border>
    
    <!--Well Selection-->
    <Border VerticalAlignment="top"
            HorizontalAlignment="Left"
            Margin="800,420,0,0">
      <StackPanel>

        <TextBlock Text="{lang:Loc Motors_Man_Well_Sel}"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="28"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Center"
                 VerticalAlignment="Top"
                 Margin="0,0,0,10"/>

        <Border BorderThickness="1"
                BorderBrush="{StaticResource HaltButtonBackgroundHighlighted}"
                Margin="0,0,0,3"
                Width="240"
                Height="60">
          <StackPanel HorizontalAlignment="Center"
                    Orientation="Horizontal">
            <RadioButton Content="96"
                       Style="{StaticResource RadioButtonHorizontalThemeStyle}"
                       IsChecked="True"
                       Width="120"
                       Command="{Binding ChangeAmountOfWellsCommand}">
              <RadioButton.CommandParameter>
                <System:Int32>96</System:Int32>
              </RadioButton.CommandParameter>
            </RadioButton>
            <RadioButton Content="384"
                       Style="{StaticResource RadioButtonHorizontalThemeStyle}"
                       Width="120"
                       Command="{Binding ChangeAmountOfWellsCommand}">
              <RadioButton.CommandParameter>
                <System:Int32>384</System:Int32>
              </RadioButton.CommandParameter>
            </RadioButton>
          </StackPanel>
        </Border>

        <StackPanel Orientation="Horizontal"
                  HorizontalAlignment="Center"
                  Height="60"
                  Width="240">
          <dx:DropDownButton Width="120"
                        FontSize="40"
                        FontFamily="{StaticResource AppTextFont}"
                        Foreground="{StaticResource AppTextColor}"
                        Content="{Binding SelectedWellRow}">
            <dxb:PopupMenu ItemLinksSource="{Binding WellRowButtonItems}">
              <dxb:PopupMenu.ItemTemplate>
                <DataTemplate>
                  <ContentControl>
                    <dxb:BarButtonItem Content="{Binding Content}"  
                                   ItemClick="{DXEvent 'Click((int)1)'}"/>

                  </ContentControl>
                </DataTemplate>
              </dxb:PopupMenu.ItemTemplate>
            </dxb:PopupMenu>
          </dx:DropDownButton>
          <dx:DropDownButton Width="120"
                        FontSize="40"
                        FontFamily="{StaticResource AppTextFont}"
                        Foreground="{StaticResource AppTextColor}"
                        Content="{Binding SelectedWellColumn}">
            <dxb:PopupMenu ItemLinksSource="{Binding WellColumnButtonItems}">
              <dxb:PopupMenu.ItemTemplate>
                <DataTemplate>
                  <ContentControl>
                    <dxb:BarButtonItem Content="{Binding Content}"  
                                     ItemClick="{DXEvent 'Click((int)2)'}"/>
                  </ContentControl>
                </DataTemplate>
              </dxb:PopupMenu.ItemTemplate>
            </dxb:PopupMenu>
          </dx:DropDownButton>
        </StackPanel>

        <Button Content="{lang:Loc Motors_GoTo_Well}"
                Command="{Binding GoToWellButtonClickCommand}"
                Style="{StaticResource ButtonStyle}"
                Height="60" Width="240"
                Margin="0,3,0,0"/>
      </StackPanel>
    </Border>
    
  </Grid>
</UserControl>
